PREFIX safuncs: <http://www.opengis.net/def/function/aggregate/geosparql/>
PREFIX funcs: <http://www.opengis.net/def/function/geosparql/>

funcs: a skos:Collection ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:member
        safuncs:bbox,
        safuncs:boundingcircle,
        safuncs:centroid,
        safuncs:concat_lines,
        safuncs:concavehull,
        safuncs:convexhull,
        safuncs:union;
    skos:prefLabel "GeoSPARQL Spatial Aggregate Functions" .
    
funcs: a skos:Collection ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:member
        funcs:minx,
        funcs:miny,
        funcs:minz,
        funcs:maxx,
        funcs:maxy,
        funcs:maxz;
    skos:prefLabel "GeoSPARQL Functions" .
