PREFIX aggfuncs: <http://www.opengis.net/def/function/aggregate/geosparql/>
PREFIX funcs: <http://www.opengis.net/def/function/geosparql/>

funcs: a skos:Collection ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:member
        aggfuncs:bbox,
        aggfuncs:boundingcircle,
        aggfuncs:centroid,
        aggfuncs:concat_lines,
        aggfuncs:concavehull,
        aggfuncs:convexhull,
        aggfuncs:union;
    skos:prefLabel "GeoSPARQL Aggregate Functions" .
    
funcs: a skos:Collection ;
    dcterms:source <http://www.opengis.net/doc/geosparql/1.1> ;
    rdfs:seeAlso <http://www.opengis.net/doc/geosparql/1.1> ;
    skos:member
        funcs:minx,
        funcs:miny,
        funcs:minz,
        funcs:maxx,
        funcs:maxy,
        funcs:maxz;
    skos:prefLabel "GeoSPARQL Functions" .
